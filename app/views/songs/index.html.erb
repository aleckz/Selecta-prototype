<script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.js"></script>

<div ng-controller="SongsCtrl as slctaController">
  <form>
    <input type="text" ng-model="slctaController.searchTerm">
    <button ng-click="slctaController.doSearch()">Search</button>
  </form>
  <ul ng-repeat="song in slctaController.songs">
    <li>
      <a ng-href="#/track/{{song.id}}" ng-click="slctaController.clearList()">{{song.title}}</a>
      {{song.artwork_id}}
    </li>
  </ul>
</div>

<div ng-controller="TrackCtrl as TC">
    <ul ng-repeat="song in TC.songs">
      <li>{{song.soundcloud_id}}</li>
    </ul>
</div>


  <div>
    <ui-view></ui-view>
  </div>
