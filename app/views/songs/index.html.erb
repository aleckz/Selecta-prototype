<h1>Selecta</h1>

<div ng-controller="SongsCtrl as slctaController">
<form>
  <input type="text" ng-model="slctaController.searchTerm">
  <button ng-click="slctaController.doSearch()">Search</button>
</form>
<ul ng-repeat="song in slctaController.songs">
  <li>
    {{song.title}}
    {{song.likes_count}}
  </li>
</ul>
{{slctaController.searchTerm}}
</div>
